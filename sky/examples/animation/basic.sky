<sky>

<div id="test">
Hello, Sky
</div>
<script>
window.addEventListener('load', function() {
  var div = document.getElementById('test');
  div.animate([
    { backgroundColor: 'red', opacity: 1 },
    { backgroundColor: 'green', opacity: 0}],
    {
      duration: 1000,
      iterations: 2
    });
});
</script>
</sky>
